		{
			"name": "Get By Genre (Comedy)",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Statut 200 OK\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Réponse est un array\", function () {\r",
							"    let jsonData = pm.response.json();\r",
							"    pm.expect(jsonData).to.be.an(\"array\");\r",
							"});\r",
							"\r",
							"pm.test(\"Tout les éléments de l'array contiennent le genre 'comedy'\", function () {\r",
							"    let jsonData = pm.response.json();\r",
							"    let containComedy = true;\r",
							"    jsonData.forEach((media) => {\r",
							"        if(!media.genres) {\r",
							"            containComedy = false;\r",
							"        }\r",
							"        containComedy = containComedy && media.genres.includes(\"Comedy\");\r",
							"    })\r",
							"    pm.expect(containComedy).to.eql(true);\r",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
